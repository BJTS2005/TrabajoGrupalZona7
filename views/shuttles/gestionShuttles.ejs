<!DOCTYPE html>
<html lang="es">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.3.0/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet">
    <title>Gestión de Servicios de Transporte</title>
    <link rel="stylesheet" href="/stylesheets/campus_style/servicios_trans/gestion-servicios-trans.css">
</head>

<body>
    <%- include ../../views/layout/cabecera.ejs %>
        <% const columnas=[ { nombre: "ID" , campo: "sti_id" , tipo: "texto" }, { nombre: "Tipo de Emision" ,
            campo: "TipoEmision" , subCampo: "tpe_detalle" , tipo: "texto" }, { nombre: "Descripsion" ,
            campo: "sti_descripcion" , tipo: "resaltado" }, { nombre: "Promedio Pasajerps" , campo: "sti_prom_pasajeros"
            , tipo: "texto" }, { nombre: "Viajes al día" , campo: "sti_tot_viajes_dia" , tipo: "texto" }, {
            nombre: "Fecha de Registro" , campo: "sti_fecha_registro" , tipo: "texto" } ]; %>

            <div class="container mt-5">
                <div class="page-header d-flex justify-content-between align-items-center">
                    <div>
                        <h1 class="page-title">Gestión de Servicios de Transporte</h1>
                        <p class="page-subtitle">Administra los servicios de transporte del campus</p>
                    </div>
                    <button class="add-service-btn" data-bs-toggle="modal" data-bs-target="#registro-servicio-modal">
                        <i class="fas fa-plus me-2"></i>Registrar Nuevo Servicio
                    </button>
                </div>

                <!-- Formulario de Filtros -->
                <form method="GET" action="/shuttles/gestionar/<%= currentCampus %>" class="mb-4">
                    <div class="row g-2 align-items-end">
                        <!-- Filtro por Tipo de Emisión -->
                        <div class="col">
                            <label for="filterTipoEmision" class="form-label">Tipo de Emisión</label>
                            <select id="filterTipoEmision" name="tipoEmision" class="form-select">
                                <option value="">Todos</option>
                                <% tiposEmision.forEach(emision=> { %>
                                    <option value="<%= emision.tpe_id %>" <%=emision.tpe_id===tipoEmision ? "selected"
                                        : "" %>>
                                        <%= emision.tpe_detalle %>
                                    </option>
                                    <% }); %>
                            </select>
                        </div>

                        <!-- Filtro por Rango de Fechas -->
                        <div class="col">
                            <label for="fechaInicio" class="form-label">Fecha Inicio</label>
                            <input type="date" id="fechaInicio" name="fechaInicio" class="form-control"
                                value="<%= fechaInicio %>">
                        </div>
                        <div class="col">
                            <label for="fechaFin" class="form-label">Fecha Fin</label>
                            <input type="date" id="fechaFin" name="fechaFin" class="form-control"
                                value="<%= fechaFin %>">
                        </div>

                        <!-- Ordenar por -->
                        <div class="col">
                            <label for="orderBy" class="form-label">Ordenar por</label>
                            <select id="orderBy" name="orderBy" class="form-select">
                                <option value="">Por Defecto</option>
                                <option value="sti_prom_pasajeros" <%=orderBy==="sti_prom_pasajeros" ? "selected" : ""
                                    %>>
                                    Promedio de Pasajeros
                                </option>
                                <option value="sti_tot_viajes_dia" <%=orderBy==="sti_tot_viajes_dia" ? "selected" : ""
                                    %>>
                                    Viajes al Día
                                </option>
                                <option value="sti_fecha_registro" <%=orderBy==="sti_fecha_registro" ? "selected" : ""
                                    %>>
                                    Fecha de Registro
                                </option>
                            </select>
                        </div>

                        <!-- Dirección del Orden -->
                        <div class="col">
                            <label for="orderDir" class="form-label">Dirección</label>
                            <select id="orderDir" name="orderDir" class="form-select">
                                <option value="ASC" <%=orderDir==="ASC" ? "selected" : "" %>>Ascendente</option>
                                <option value="DESC" <%=orderDir==="DESC" ? "selected" : "" %>>Descendente</option>
                            </select>
                        </div>

                        <!-- Botón de Filtrar -->
                        <div class="col">
                            <button type="submit" class="btn btn-primary">Filtrar</button>
                        </div>
                    </div>
                </form>

                <%- include('../../views/parciales/tablaReutilizable.ejs', { columnas, datos: shuttles }) %>

                <!-- Modal Registrar-->
                <div class="modal fade" id="registro-servicio-modal" tabindex="-1">
                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content modal-rst-bg-dark">
                            <div class="modal-header border-0 px-4 pt-4">
                                <div>
                                    <h5 class="modal-title text-success fs-6">Nuevo Servicio</h5>
                                </div>
                                <button type="button" class="btn-close btn-close-white opacity-75" data-bs-dismiss="modal" aria-label="Close"></button>
                            </div>
                            <div class="modal-body px-4">
                                <h4 class="text-white mb-4">Registrar Servicio de Transporte</h4>
                                <form action="/shuttles/gestionar/<%= currentCampus %>" method="post" id="service-form">
                                    <div class="mb-4">
                                        <label for="sti_id" class="form-label modal-rst-label">
                                            <i class="fa fa-hashtag icono-modal-rst me-2"></i>ID del Servicio
                                        </label>
                                        <input type="text" id="sti_id" name="sti_id" class="form-control modal-rst-input" placeholder="Ej: SRV-001" maxlength="5" required>
                                    </div>
                
                                    <div class="mb-4">
                                        <label for="camp_id" class="form-label modal-rst-label">
                                            <i class="fa fa-university icono-modal-rst me-2"></i>Campus
                                        </label>
                                        <select id="camp_id" name="camp_id" class="form-select modal-rst-input" required>
                                            <option value="">Seleccionar campus</option>
                                            <% campuses.forEach(campus => { %>
                                                <option value="<%= campus.camp_id %>" <%=(currentCampus===campus.camp_id) ? 'selected' : '' %>>
                                                    <%= campus.camp_nom %>
                                                </option>
                                            <% }); %>
                                        </select>
                                    </div>
                
                                    <div class="mb-4">
                                        <label for="tpe_id" class="form-label modal-rst-label">
                                            <i class="fa fa-random icono-modal-rst me-2"></i>Tipo de Emisión
                                        </label>
                                        <select id="tpe_id" name="tpe_id" class="form-select modal-rst-input" required>
                                            <option value="">Seleccionar tipo</option>
                                            <% tiposEmision.forEach(tipo => { %>
                                                <option value="<%= tipo.tpe_id %>"><%= tipo.tpe_detalle %></option>
                                            <% }); %>
                                        </select>
                                    </div>
                
                                    <div class="mb-4">
                                        <label for="sti_descripcion" class="form-label modal-rst-label">
                                            <i class="fa fa-file-text icono-modal-rst me-2"></i>Descripción del servicio
                                        </label>
                                        <textarea id="sti_descripcion" name="sti_descripcion" class="form-control modal-rst-input" rows="3" placeholder="Describe el servicio de transporte" required></textarea>
                                    </div>
                
                                    <div class="row">
                                        <div class="col-6 mb-4">
                                            <label for="sti_prom_pasajeros" class="form-label modal-rst-label">
                                                <i class="fa fa-users icono-modal-rst me-2"></i>Aproximado de pasajeros
                                            </label>
                                            <input type="number" id="sti_prom_pasajeros" name="sti_prom_pasajeros" class="form-control modal-rst-input" placeholder="Ej: 50" required min="0">
                                        </div>
                                        <div class="col-6 mb-4">
                                            <label for="sti_tot_viajes_dia" class="form-label modal-rst-label">
                                                <i class="fa fa-bus icono-modal-rst me-2"></i>Viajes al día
                                            </label>
                                            <input type="number" id="sti_tot_viajes_dia" name="sti_tot_viajes_dia" class="form-control modal-rst-input" placeholder="Ej: 10" required min="0">
                                        </div>
                                    </div>
                                    
                                    <div class="modal-footer border-0 px-4 pb-4">
                                        <button type="button" class="btn modal-rst-btn-cancel" data-bs-dismiss="modal">Cancelar</button>
                                        <button type="submit" class="btn modal-rst-btn-success px-4">Registrar Servicio</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>




                <!-- Modal Editar-->









                <div class="modal fade" id="modalEditarShuttle" tabindex="-1">
                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content modal-rst-bg-dark">
                            <div class="modal-header border-0 px-4 pt-4">
                                <div>
                                    <h5 class="modal-title text-success fs-6">Editar Servicio</h5>
                                </div>
                                <button type="button" class="btn-close btn-close-white opacity-75" data-bs-dismiss="modal" aria-label="Close"></button>
                            </div>
                            <div class="modal-body px-4">
                                <h4 class="text-white mb-4">Editar Servicio de Transporte</h4>
                                <form action="/shuttles/gestionar/<%= currentCampus %>/actualizar" method="post" id="edit-service-form">

                                    <input type="hidden" id="sti_id_editar" name="sti_id">
                
                                    <div class="mb-4">
                                        <label for="tpe_id_editar" class="form-label modal-rst-label">
                                            <i class="fa fa-random icono-modal-rst me-2"></i>Tipo de Emisión
                                        </label>
                                        <select id="tpe_id_editar" name="tpe_id" class="form-select modal-rst-input" required>
                                            <% tiposEmision.forEach(tipo => { %>
                                                <option value="<%= tipo.tpe_id %>">
                                                    <%= tipo.tpe_detalle %>
                                                </option>
                                            <% }); %>
                                        </select>
                                    </div>
                
                                    <div class="mb-4">
                                        <label for="sti_descripcion_editar" class="form-label modal-rst-label">
                                            <i class="fa fa-file-text icono-modal-rst me-2"></i>Descripción del servicio
                                        </label>
                                        <textarea id="sti_descripcion_editar" name="sti_descripcion" class="form-control modal-rst-input" rows="3" required></textarea>
                                    </div>
                
                                    <div class="row">
                                        <div class="col-6 mb-4">
                                            <label for="sti_prom_pasajeros_editar" class="form-label modal-rst-label">
                                                <i class="fa fa-users icono-modal-rst me-2"></i>Aproximado de pasajeros
                                            </label>
                                            <input type="number" id="sti_prom_pasajeros_editar" name="sti_prom_pasajeros" class="form-control modal-rst-input" required min="0">
                                        </div>
                                        <div class="col-6 mb-4">
                                            <label for="sti_tot_viajes_dia_editar" class="form-label modal-rst-label">
                                                <i class="fa fa-bus icono-modal-rst me-2"></i>Viajes al día
                                            </label>
                                            <input type="number" id="sti_tot_viajes_dia_editar" name="sti_tot_viajes_dia" class="form-control modal-rst-input" required min="0">
                                        </div>
                                    </div>
                                    
                                    <div class="modal-footer border-0 px-4 pb-4">
                                        <button type="button" class="btn modal-rst-btn-cancel" data-bs-dismiss="modal">Cancelar</button>
                                        <button type="submit" class="btn modal-rst-btn-success px-4">Guardar Cambios</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                


                
                    




            </div>




